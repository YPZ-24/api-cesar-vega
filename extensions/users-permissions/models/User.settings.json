{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true,
      "unique": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": true
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false
    },
    "edad": {
      "type": "integer"
    },
    "fechaNacimiento": {
      "type": "date"
    },
    "telefono": {
      "type": "biginteger"
    },
    "saldo": {
      "type": "decimal",
      "default": 0
    },
    "codigos": {
      "collection": "codigos",
      "via": "usuarioPropietario"
    },
    "codigoReferido": {
      "model": "codigos",
      "via": "usuarioReferido"
    },
    "citas": {
      "via": "usuario",
      "collection": "citas"
    },
    "videos_usuarios": {
      "collection": "videos-usuario",
      "via": "usuario"
    },
    "customerId": {
      "type": "string"
    },
    "dispositivos": {
      "collection": "dispositivos",
      "via": "user"
    },
    "cursos": {
      "collection": "cursos",
      "via": "users",
      "dominant": true
    },
    "imagenPerfil": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "cliente": {
      "type": "boolean",
      "default": false
    }
  }
}
