{
  "kind": "collectionType",
  "collectionName": "citas",
  "info": {
    "name": "Citas",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "fecha": {
      "type": "datetime",
      "required": true
    },
    "asunto": {
      "type": "string",
      "required": true
    },
    "usuario": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "citas"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "CREADA",
        "PAGADA",
        "AGENDADA"
      ],
      "default": "CREADA"
    },
    "enlace": {
      "type": "string"
    }
  }
}
